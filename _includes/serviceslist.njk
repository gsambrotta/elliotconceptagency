{% for service in serviceslist %}
    <div class="col-12 col-md-6 col-lg-4 item">
        <div class="card featured">
            <i class="icon {{ service.icon }}"></i>
            <div class='card-last-item'>
                <h4>{{ service.title }}</h4>
                <p>{{ service.description }}</p>
            </div>
        </div>
    </div>
{% endfor %}